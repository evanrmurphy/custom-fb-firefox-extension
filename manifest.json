{

  "manifest_version": 2,
  "name": "custom-fb-extension",
  "version": "1.3",

  "description": "Evan's customizations of mobile facebook.com on Firefox Android.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["extension.js"],
      "css": ["extension.css"]
    }
  ]

}
